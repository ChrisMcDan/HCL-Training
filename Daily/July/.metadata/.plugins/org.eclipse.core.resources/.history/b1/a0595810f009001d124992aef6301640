package com.daily.July_22_2022;

public class OuterClass
{
	private int x = 34;

	class Inner
	{
		void display()
		{
			System.out.println("The private value of outerclass variable x is: " + x);
		}
	}

	public static void main(String[] args)
	{
		System.out.println("Hello World!");
	}
}
